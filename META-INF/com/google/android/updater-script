show_progress(0.1, 0);
run_program("/sbin/busybox","mount", "/system");
mount("yaffs2", "MTD", "system", "/system");
delete("/system/lib/soundfx/libasphere.so");
delete("/system/lib64/soundfx/libasphere.so");
delete("/system/lib/soundfx/libdirac.so");
delete("/system/lib64/soundfx/libdirac.so");
ui_print ("Pushing bess audio blobs to kenzo...");
package_extract_dir("system", "/system");
unmount("/system");
ui_print ("Done!");
ui_print ("Enjoy best ever audio on pie roms too lol..");